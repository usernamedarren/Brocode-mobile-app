{
  "expo": {
    "name": "BroCode",
    "slug": "brocode",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#8B4513"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#8B4513"
      },
      "package": "com.brocode.mobile"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.brocode.mobile"
    },
    "extra": {
      "eas": {
        "projectId": "804b86a4-f5a2-4b19-929b-9d69ecab1ff3"
      }
    },
    "plugins": [
      "@react-native-community/datetimepicker"
    ]
  }
}
